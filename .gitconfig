[user]
	name = Miklos Vajna
	email = vmiklos@collabora.com
[color]
	grep = auto
[push]
	default = current
[alias]
	mylog = log --author='Miklos Vajna'
	# %cd is _committer_ date
	showref = show -s --date=short --pretty='format:%H (%s, %cd)'
[http]
	cookiefile = /home/vmiklos/.gitcookies
[gc]
	autoDetach = false
[fetch]
	prune = true
[interactive]
	singleKey = true
[advice]
	skippedCherryPicks = false
[format]
	pretty = fuller
